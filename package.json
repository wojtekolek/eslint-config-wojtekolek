{
  "name": "@wojtekolek/eslint-config",
  "version": "3.2.1",
  "description": "ESLint presets and prettier config",
  "type": "module",
  "repository": "git@github.com:wojtekolek/eslint-config-wojtekolek.git",
  "scripts": {
    "lint": "eslint \"**/*.{js,mjs}\"",
    "format": "prettier --write \"**/*.{js,mjs}\""
  },
  "keywords": [
    "eslint",
    "eslint config",
    "prettier"
  ],
  "exports": {
    ".": "./src/node.mjs",
    "./node": "./src/node.mjs",
    "./nextjs": "./src/nextjs.mjs",
    "./react": "./src/react.mjs",
    "./react-native": "./src/react-native.mjs",
    "./prettier": "./src/prettier.mjs"
  },
  "author": "Wojtek Olek",
  "license": "MIT",
  "dependencies": {
    "@eslint/compat": "^1.2.6",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@next/eslint-plugin-next": "^15.1.6",
    "eslint-config-next": "^15.1.6",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-restricted-globals": "^0.2.0",
    "globals": "^15.14.0",
    "typescript-eslint": "^8.22.0"
  },
  "devDependencies": {
    "eslint": "^9.19.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "prettier": ">=3.2.5",
    "typescript": ">=5.4.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "prettier": "./src/prettier.mjs"
}
