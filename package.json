{
  "name": "@wojtekolek/eslint-config",
  "version": "3.0.0",
  "description": "ESLint presets and prettier config",
  "type": "module",
  "repository": "git@github.com:wojtekolek/eslint-config-wojtekolek.git",
  "scripts": {
    "format": "prettier --write \"**/*.{js,mjs}\""
  },
  "keywords": [
    "eslint",
    "eslint config",
    "prettier"
  ],
  "exports": {
    ".": "./src/node.js",
    "./node": "./src/node.js",
    "./nextjs": "./src/nextjs.js",
    "./react": "./src/react.js",
    "./react-native": "./src/react-native.js",
    "./prettier": "./src/prettier.js"
  },
  "author": "Wojtek Olek",
  "license": "MIT",
  "dependencies": {
    "@rushstack/eslint-patch": "^1.10.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint-config-next": "^14.1.4",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-restricted-globals": "^0.2.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0",
    "prettier": ">=3.2.5",
    "typescript": ">=5.4.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "prettier": "./src/prettier.js"
}
